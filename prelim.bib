	urldate = {2014-04-15},
	date = {2010-04-06},
	langid = {english},
	note = {{PMID:} 20231444},
	keywords = {bacterial forensics, human microbiome, microbial ecology, pyrosequencing, skin microbiology},
	file = {Full Text PDF:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/AN9IHIAT/Fierer et al. - 2010 - Forensic identification using skin bacterial commu.pdf:application/pdf;Snapshot:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/2E7UQPCH/6477.html:text/html}
