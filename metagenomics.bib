	keywords = {chrismetagenomics},
	file = {Full Text PDF:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/DFQF7SZF/DeSantis et al. - 2006 - Greengenes, a Chimera-Checked 16S rRNA Gene Databa.pdf:application/pdf;Snapshot:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/QJPF65IP/5069.html:text/html}
