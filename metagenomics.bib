	keywords = {chrismetagenomics},
	file = {Full Text PDF:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/7NF5DPF5/Schloss et al. - 2009 - Introducing mothur Open-Source, Platform-Independ.pdf:application/pdf;Snapshot:/Users/cmhill/Library/Application Support/Zotero/Profiles/zglkxv0v.default/zotero/storage/9K7QKKMC/7537.html:text/html}
